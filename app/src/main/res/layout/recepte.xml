<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
<LinearLayout
        android:layout_width="match_parent"
android:padding="25dp"
              android:orientation="vertical"
              android:layout_height="match_parent">
    <TextView android:layout_width="match_parent"
              android:text="Recept"
              android:textStyle="bold"
              android:textSize="75sp"
              android:gravity="center_horizontal"
              android:layout_height="wrap_content"/>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
                  android:orientation="horizontal"
                  android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="TransactionId"
                  android:layout_marginStart="25dp"
         android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/TransactionId"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
        android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="CountryCode"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getCountryCode"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="CustomIdentifier"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getCustomIdentifier"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="DeliveredAmount"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:text="@string/app_name"
                  android:id="@+id/getDeliveredAmount"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="Discount"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getDiscount"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="OperatorName"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getOperatorName"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="OperatorTransactionId"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getOperatorTransactionId"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="SenderPhone"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getSenderPhone"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="RequestedAmount"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getRequestedAmount"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="RecipientPhone"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:id="@+id/getRecipientPhone"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:background="@android:color/holo_orange_light"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content"
                  android:text="@string/app_name"
                  android:layout_marginStart="25dp"
                  android:layout_height="wrap_content"/>
        <TextView android:layout_width="wrap_content"
                  android:layout_alignParentEnd="true"
                  android:text="@string/app_name"
                  android:layout_marginEnd="25dp"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>
</LinearLayout>
</RelativeLayout>